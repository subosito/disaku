.row
  .col-md-6
    = simple_form_for @search, url: transactions_path, method: :get, html: { class: 'form-inline' } do |f|
      .input-daterange{ data: { behaviour: 'datepicker' } }
        .col-md-4
          = f.input_field :transaction_date_gteq
        .col-md-4
          = f.input_field :transaction_date_lteq
      = f.button :submit, 'Search', class: 'btn btn-default'

