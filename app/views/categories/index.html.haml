= link_to 'New Category', new_category_path, class: 'btn btn-default'

%table.table
  %tr
    %th Name
    %th Category Type
    %th Monthly Income
    %th Montly Expense
  - categories.each do |category|
    %tr
      %td= category.name
      %td= category.label
      %td= category.monthly_income
      %td= category.monthly_expense
